{
  "name": "react-anchorify-text",
  "version": "2.1.1",
  "description": "React component which render text with anchored url(s)",
  "main": "lib/index.js",
  "scripts": {
    "browser": "browser-sync start --files example/* --server example",
    "watch:example": "watchify example/app.js -dv -o example/bundle.js",
    "start:example": "npm run watch:example & npm run browser",
    "test:local": "karma start",
    "test": "./node_modules/.bin/karma start --browsers Firefox --single-run",
    "clean": "rimraf lib",
    "build": "babel src --out-dir lib"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/georgeosddev/react-anchorify-text"
  },
  "keywords": [
    "react",
    "react-component",
    "url",
    "anchor",
    "link"
  ],
  "author": "Takeharu.Oshida",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/georgeosddev/react-anchorify-text/issues"
  },
  "homepage": "https://github.com/georgeosddev/react-anchorify-text",
  "devDependencies": {
    "babel-cli": "^6.3.17",
    "babel-core": "^6.3.26",
    "babel-eslint": "^7.2.3",
    "babel-preset-es2015": "^6.3.13",
    "babel-preset-react": "^6.3.13",
    "babelify": "^7.2.0",
    "browser-sync": "^2.9.12",
    "browserify": "^14.4.0",
    "chai": "^4.0.1",
    "eslint": "^3.19.0",
    "eslint-plugin-react": "^7.0.1",
    "karma": "^1.7.0",
    "karma-browserify": "^5.1.1",
    "karma-chai": "^0.1.0",
    "karma-chrome-launcher": "^2.1.1",
    "karma-cli": "1.0.1",
    "karma-firefox-launcher": "^1.0.1",
    "karma-mocha": "^1.3.0",
    "karma-safari-launcher": "^1.0.0",
    "karma-spec-reporter": "0.0.31",
    "mocha": "^3.4.2",
    "react": "^15.0.0",
    "react-dom": "^15.0.0",
    "react-addons-test-utils": "^15.5.1",
    "rimraf": "^2.5.0",
    "watchify": "^3.6.0"
  },
  "dependencies": {
    "prop-types": "^15.5.10",
    "linkify-it": "^2.0.0",
    "tlds": "^1.145.0"
  },
  "peerDependencies": {
    "react": "^15.0.0",
    "react-dom": "^15.0.0"
  },
  "browserify": {
    "transform": [
      [
        "babelify",
        {
          "compact": false,
          "presets": [
            "es2015",
            "react"
          ]
        }
      ]
    ]
  }
}
